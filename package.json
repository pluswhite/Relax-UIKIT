{
  "name": "relax-ui-kit",
  "version": "0.3.0",
  "description": "A UI KIT based on React with Typescript, Jest, Webpack, Babel .etc.",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "build": "webpack --mode production --config build/webpack.config.ts",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint ./src/**/*.{ts,tsx} --quiet",
    "lint:style": "stylelint ./src/**/*.scss",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public -o storybook-static",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write .",
      "git add"
    ],
    "./src/**/*.scss": "stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricThsi/Relax-UIKIT.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "typescript",
    "eslint",
    "prettier",
    "webpack",
    "jest"
  ],
  "author": "PlusWhite",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricThsi/Relax-UIKIT/issues"
  },
  "homepage": "https://github.com/EricThsi/Relax-UIKIT#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^6.0.18",
    "@storybook/addon-docs": "^6.0.18",
    "@storybook/addon-links": "^6.0.18",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.18",
    "@testing-library/dom": "^7.20.2",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.6",
    "@testing-library/user-event": "^12.0.11",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.13.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm": "^6.14.6",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-twbs-bootstrap": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
